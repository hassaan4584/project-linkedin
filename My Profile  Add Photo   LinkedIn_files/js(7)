/**
 * @fileoverview
 * This package provides classes related
 * to the media server communication.
 */

if( Lui != null && Lui.Media == null) {

  /**
   * Namespace for classes etc.
   */
  Lui.Media = {}; // class namespace
}


if( Lui.Media != null && Lui.Media.Cropper == null) {

  /**
   * 
   * @constructor
   */
  Lui.Media.Cropper = function( initialMaskDim, width, height) {
	this.pictureWidth = width || 80;
	this.pictureHeight = height || 80;
    this._wireCropperElements();
    this._moveAndResizeCallback = null;
    this._minMaskDim = { width: this.pictureWidth, height: this.pictureHeight };
    this._initialMaskDim = initialMaskDim;
  };
  
  Lui.Media.Cropper.prototype.PREVIEW_IMG_ID = "preview-img";
  Lui.Media.Cropper.prototype.CANVAS_IMG_ID = "canvas-img";
  Lui.Media.Cropper.prototype.CANVAS_MASK_ID = "canvas-mask";
  Lui.Media.Cropper.prototype.CANVAS_MASK_HANDLE_ID = "canvas-mask-handle";

  Lui.Media.Cropper.prototype.MASK_CHANGE_DELTA_MIN = 2; // needs to be even number since it will be divided by 2
  Lui.Media.Cropper.prototype.MASK_CHANGE_DELTA_MAX = 10; // needs to be even an number since it will be divided by 2

  Lui.Media.Cropper.prototype._numberComparator = function(a, b) {
    return a - b;
  };

  /**
   * This function is called when the user moves and/or
   * resizes the mask.
   */
  Lui.Media.Cropper.prototype.setMoveAndResizeCallback = function( moveAndResizeCallback) {
  	this._moveAndResizeCallback = moveAndResizeCallback;
  };

  /**
   * Set minimum dimensions for cropping mask. The mask cannot
   * be made smaller than that.
   * @param dim object containing width and height
   */
  Lui.Media.Cropper.prototype.setMinimumMaskDimensions = function( dim) {
    this._minMaskDim = dim;
  };

  /**
   * Wire a given element with the given handler and add a
   * reference to this cropper to the element for later
   * retrieval.
   * @param el element to wire
   * @param handler handler to set, e.g. "click" for onclick.
   * @param func function reference to assign to handler
   */
  Lui.Media.Cropper.prototype._wire = function(el, handler, func) {
    el = YAHOO.util.Dom.get(el);
    el.cropper = this;
    el["on" + handler] = func;
  };

  /**
   * Wire cropper and initialize html elements. This is called only
   * once when the cropper dialog is created.
   */
  Lui.Media.Cropper.prototype._wireCropperElements = function() {
    // retrieve and cache frequently accessed object references
    this._canvasMask = YDom.get(this.CANVAS_MASK_ID);
    this._canvasMaskHandle = YDom.get(this.CANVAS_MASK_HANDLE_ID);

    this._wire(this._canvasMask, "mouseover", this._handleMouseOver);
    this._wire(this._canvasMask, "mouseout", this._handleMouseOut);
  };
  
  /**
   * Return current cropping settings like top, left, width and height.
   * @return map containing style settings
   */
  Lui.Media.Cropper.prototype.getCroppingInfo = function() {
    var ddHelper = this._ddHelper;
    var info = ddHelper.getElementDimensions( this._canvasMask);
    var scale = ddHelper.container.dimension;
    info.scale_w = scale.width;
    info.scale_h = scale.height;
    return info;
  };
  
  /**
    Validate if given object is a valid dimension object
   * with top, left, width and height property. We do not
   * validate whether the mask would stick out to the left
   * or bottom.
   * 
   * @param dim dimension object to validate
   * @return true if valid, false otherwise
   */
  Lui.Media.Cropper.prototype.isValidDim = function( dim) {
    if( dim == null || dim.top == null || dim.left == null || dim.width == null || dim.height == null)
      return false;
    
    if( dim.top < 0 || dim.left < 0)
      return false;
    
    if( dim.width < this._minMaskDim.width || dim.height < this._minMaskDim.height)
      return false;
    
    return true;
  };
  
  /**
   * This function is called by the ImagePreloader once the image
   * to crop has been downloaded. Otherwise we would not be able 
   * to determine the image size, which we need to keep the crop
   * mask size in check. The function is executed using the cropper
   * object as context. That allows us to use <code>this</code>.
   * 
   * @param images array of images attempted to load, in this case one image
   * @param loadedCount number of images actually loaded, in this case one image
   */
  Lui.Media.Cropper.prototype.initDragDrop = function(images, loadedCount) {
  	var image = images[0];
    var ddHelper = new Lui.Media.Cropper.DDHelper( { width: image.width, height: image.height }, {minWidth: this.pictureWidth, minHeight: this.pictureHeight});
    ddHelper.setBorderWidth( ddHelper.pixelStyle2Number(this._canvasMask, "borderTopWidth"));
    ddHelper.setMinimumMaskDimensions( this._minMaskDim);
    ddHelper.setMoveAndResizeCallback( this._moveAndResizeCallback);

    if( this.isValidDim( this._initialMaskDim))
      ddHelper.setElementDimensions( this._canvasMask, this._initialMaskDim);
    else
      ddHelper.setElementDimensions( this._canvasMask, ddHelper.getDefaultMaskDimensions());
    

    // add to cropper scope
    this._ddHelper = ddHelper;
    
    // create mask elements
    new Lui.Media.Cropper.DDResize( this, ddHelper, this.CANVAS_MASK_ID, this.CANVAS_MASK_HANDLE_ID, "mask-resize");
    new Lui.Media.Cropper.DDMove( this, ddHelper, this.CANVAS_MASK_ID, "mask-drag").addInvalidHandleId( this.CANVAS_MASK_HANDLE_ID);
    
    // create preview adjuster
    this._previewAdjuster = new Lui.Media.Cropper.PreviewAdjuster( this, ddHelper, image);
    this._previewAdjuster.adjust();
    
    // set up keyboard event handling to switch scaling algorithm
    var keyMap = {};
    keyMap[YAHOO.util.KeyListener.KEY.UP] = this._moveMaskUp;
    keyMap[YAHOO.util.KeyListener.KEY.LEFT] = this._moveMaskLeft;
    keyMap[YAHOO.util.KeyListener.KEY.DOWN] = this._moveMaskDown;
    keyMap[YAHOO.util.KeyListener.KEY.RIGHT] = this._moveMaskRight;
    keyMap[107] = this._expandMask; // key PLUS
    keyMap[109] = this._contractMask; // key MINUS

    var _this = this;
    
    window.document.onkeydown = function(e) {
    	var key = YEvent.getCharCode( e );
    	if( key == YAHOO.util.KeyListener.KEY.ALT)
    	  ddHelper.shiftKeyDown = true; // access ddHelper via closure
    	else {
    	  var func = keyMap[key];
    	  if( func)
    	    func.apply( _this);
    	}
    };
    
    window.document.onkeypress = function(e) {
  	  var func = keyMap[ YEvent.getCharCode( e ) ];
  	  if( func)
        func.apply( _this);
    };
    
    window.document.onkeyup = function(e) {
    	if( YEvent.getCharCode( e ) == 18 ) {
    	  ddHelper.shiftKeyDown = false; // access ddHelper via closure
    	}
    };
  };
  
  Lui.Media.Cropper.prototype._handleMouseOver = function() {
    this.className = "canvas-mask-over";
    this.cropper._canvasMaskHandle.className = "canvas-mask-handle-over";
  };
  
  Lui.Media.Cropper.prototype._handleMouseOut = function() {
    this.className = "canvas-mask-out";
    this.cropper._canvasMaskHandle.className = "canvas-mask-handle-out";
  };
  
  /**
   * Expand the mask size keeping the pivot point in the center
   * of the mask.
   */
  Lui.Media.Cropper.prototype._expandMask = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = ddHelper.getElementDimensions(canvasMask);
    
    // this is the offset we would like to expand on each side
    var offsetDesired = (ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN) / 2;

    // this is the offset we actually can expand on top/left
    // as well as on bottom/right. This algorithm will make
    // sure the mask stays a square.
    var offsetTopLeft = [
      maskDim.left, 
      maskDim.top, 
      offsetDesired 
    ].sort(this._cropper._numberComparator)[0];
    var offsetBottomRight = [
      containerDim.width - maskDim.left - maskDim.width,
      containerDim.height - maskDim.top - maskDim.height,
      offsetDesired
    ].sort(this._cropper._numberComparator)[0];

    // calculate the new dimensions
    var newDim = {
      top: maskDim.top - offsetTopLeft,
      left: maskDim.left - offsetTopLeft,
      width: maskDim.width + offsetTopLeft + offsetBottomRight,
      height: maskDim.height + offsetTopLeft + offsetBottomRight
    };
    ddHelper.setElementDimensions( canvasMask, newDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };
  
  /**
   * Contract the mask size keeping the pivot point in the center
   * of the mask.
   */
  Lui.Media.Cropper.prototype._contractMask = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var maskDim = ddHelper.getElementDimensions(canvasMask);

    // this is the offset we would like to expand on each side
    var offsetDesired = (ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN) / 2;
    
    if(   maskDim.width - offsetDesired < ddHelper.minWidth
       || maskDim.height - offsetDesired < ddHelper.minHeight)
      return;

    var offsetDesired2x = offsetDesired * 2;
    maskDim.top = maskDim.top + offsetDesired;
    maskDim.left = maskDim.left + offsetDesired;
    maskDim.width = maskDim.width - offsetDesired2x;
    maskDim.height = maskDim.height - offsetDesired2x;
    ddHelper.setElementDimensions( canvasMask, maskDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };

  /**
   * Move mask to the up.
   */
  Lui.Media.Cropper.prototype._moveMaskUp = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = ddHelper.getElementDimensions(canvasMask);
    
    // this is the offset we would like to expand on each side
    var offsetDesired = ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN;

    if( maskDim.top == 0)
      return;
      
    maskDim.top = maskDim.top < offsetDesired ? 0 : maskDim.top - offsetDesired;
    ddHelper.setElementDimensions( canvasMask, maskDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };
  
  /**
   * Move mask to the left.
   */
  Lui.Media.Cropper.prototype._moveMaskLeft = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = ddHelper.getElementDimensions(canvasMask);
    
    // this is the offset we would like to expand on each side
    var offsetDesired = ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN;

    if( maskDim.left == 0)
      return;
      
    maskDim.left = maskDim.left < offsetDesired ? 0 : maskDim.left - offsetDesired;
    ddHelper.setElementDimensions( canvasMask, maskDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };
  
  /**
   * Move mask to the right.
   */
  Lui.Media.Cropper.prototype._moveMaskRight = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = ddHelper.getElementDimensions(canvasMask);
    
    // this is the offset we would like to expand on each side
    var offsetDesired = ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN;

    var rightRemainder = containerDim.width - (maskDim.left + maskDim.width);
    if( rightRemainder == 0)
      return;
      
    maskDim.left = rightRemainder < offsetDesired ? maskDim.left + rightRemainder : maskDim.left + offsetDesired;
    ddHelper.setElementDimensions( canvasMask, maskDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };
  
  /**
   * Move mask to the down.
   */
  Lui.Media.Cropper.prototype._moveMaskDown = function() {
    var cropper = this instanceof Lui.Media.Cropper ? this : this.cropper;
    var canvasMask = cropper._canvasMask;
    var ddHelper = cropper._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = ddHelper.getElementDimensions(canvasMask);
    
    // this is the offset we would like to expand on each side
    var offsetDesired = ddHelper.shiftKeyDown ? cropper.MASK_CHANGE_DELTA_MAX : cropper.MASK_CHANGE_DELTA_MIN;

    var bottomRemainder = containerDim.height - (maskDim.top + maskDim.height);
    if( bottomRemainder == 0)
      return;

    maskDim.top = bottomRemainder < offsetDesired ? maskDim.top + bottomRemainder : maskDim.top + offsetDesired;
    ddHelper.setElementDimensions( canvasMask, maskDim);
    
    // finally adjust the preview panel
    cropper._previewAdjuster.adjust();
  };
  
  // -----------------------------------------
  // Drag & Drop Helper class
  // -----------------------------------------
  
  /**
   * DDHelper servers as a container for dimensional information
   * and provides convenience functions.
   * @constructor
   * @param dim dimensions of the container, which is the image size
   */
  Lui.Media.Cropper.DDHelper = function( dim, minDim) {
    this.container = {};
    this.container.dimension = { height: dim.height, width: dim.width };
    this.minWidth = minDim.minWidth;
    this.minHeight = minDim.minHeight;
    this.borderWidth = 0;
    this.borderWidth2x = 0;
    this.shiftKeyDown = false;
  };
  
  /**
   * This function is called when the user moves and/or
   * resizes the mask.
   */
  Lui.Media.Cropper.DDHelper.prototype.setMoveAndResizeCallback = function( moveAndResizeCallback) {
    this._moveAndResizeCallback = moveAndResizeCallback;
  };

  /**
   * Retrieve pixel based style property, remove the px extension
   * and return the value as number. For performance reasonse, we
   * do not perform any validation of whether the value ends with
   * the px extension.
   * 
   * @param o HTMLElement containing the pixel style
   * @param p property to extract the value for
   * @return pixel value as number
   */
  Lui.Media.Cropper.DDHelper.prototype.pixelStyle2Number = function(el, prop) {
    // this function relies on the fact that style attributes top,
    // left, height and width always have "px" as unit attached
    var s = YDom.getStyle(el, prop);
    return s.substring( 0, s.length - 2) * 1;
  };
  
  /**
   * Set minimum dimensions for cropping mask. The mask cannot
   * be made smaller than that.
   * @param dim object containing width and height
   */
  Lui.Media.Cropper.DDHelper.prototype.setMinimumMaskDimensions = function( dim) {
    if( dim == null || dim.width == null || dim.height == null)
      throw "Invalid dimension object";
    
    if( !(dim.width*1) || !(dim.height*1) )
      throw "Invalid minimum cropping mask dimensions";
      
    this.minWidth = dim.width;
    this.minHeight = dim.height;
  };
  
  Lui.Media.Cropper.DDHelper.prototype.setBorderWidth = function( width) {
    this.borderWidth = width;
    this.borderWidth2x = width * 2;
  };

  /**
   * Set size of element to given dimensions. This function takes into 
   * consideration that the width applies to the outer borders of the 
   * mask. This means that if we specify a width of 100 pixels, we need
   * to set the style with to 100 pixels minus twice the border witdh.
   * @param el element to resize
   * @param dim dimensions to resize element to  
   */
  Lui.Media.Cropper.DDHelper.prototype.setElementDimensions = function(el, dim) {
    YDom.setStyle( el, "top", dim.top + "px");
    YDom.setStyle( el, "left", dim.left + "px");
    YDom.setStyle( el, "width", (dim.width - this.borderWidth2x) + "px");
    YDom.setStyle( el, "height", (dim.height - this.borderWidth2x) + "px");
  };  
  
  Lui.Media.Cropper.DDHelper.prototype.setElementPosition = function(el, pos) {
    YDom.setStyle( el, "top", pos.top + "px");
    YDom.setStyle( el, "left", pos.left + "px");
  };  
  
  Lui.Media.Cropper.DDHelper.prototype.getDefaultMaskDimensions = function() {
    var containerDim = this.container.dimension;
    var size = Math.min(containerDim.width, containerDim.height);
    var top = 0; // (size == containerDim.height ? 0 : Math.floor((containerDim.height - size) / 2));
    var left = (size == containerDim.width ? 0 : Math.floor((containerDim.width - size) / 2));
    return { top: top, left: left, width: size, height: size };
  };
   
  /**
   * Determine dimensions of given node and return map
   * with top, left, width and height settings.
   * @param el element to determine dimensions for
   * @return map with top, left, width and height settings
   */
  Lui.Media.Cropper.DDHelper.prototype.getElementDimensions = function( el) {
    el = YAHOO.util.Dom.get(el);
      
    return { top: this.pixelStyle2Number(el, "top"),
             left: this.pixelStyle2Number(el, "left"),
             width: this.pixelStyle2Number(el, "width") + this.borderWidth2x,
             height: this.pixelStyle2Number(el, "height") + this.borderWidth2x };
  };
  
  // -----------------------------------------
  // Drag & Drop Resize class
  // -----------------------------------------

  /**
   * Extend DD for resizing the mask.
   * @constructor
   */
  Lui.Media.Cropper.DDResize = function(cropper, ddHelper, panelElId, handleElId, sGroup, config) {
    this.init(panelElId, sGroup, config);
    this.setHandleElId(handleElId);
    this._cropper = cropper;
    this._ddHelper = ddHelper;
    return this;
  };
  YAHOO.extend(Lui.Media.Cropper.DDResize, YAHOO.util.DragDrop);

  /**
   * 
   */
  Lui.Media.Cropper.DDResize.prototype.onMouseDown = function(e) {
    var panel = this.getEl();
    this._panelDim = this._ddHelper.getElementDimensions( panel);
                     
    this.startWidth = panel.offsetWidth;
    this.startHeight = panel.offsetHeight;
    this.startPos = [YAHOO.util.Event.getPageX(e),
                     YAHOO.util.Event.getPageY(e)];
  };
  
  /**
   * Event handler for resizing the cropping area. The
   * function makes sure that the mask cannot get outside
   * the image boundaries.
   */
  Lui.Media.Cropper.DDResize.prototype.onDrag = function(e) {
    var newPos = [YAHOO.util.Event.getPageX(e),
                  YAHOO.util.Event.getPageY(e)];
  
    var offsetX = newPos[0] - this.startPos[0];
    var offsetY = newPos[1] - this.startPos[1];

    // this is the offset we would like to expand on each side
    var offsetDesired = Math.max(offsetX,offsetY);

    var canvasMask = this.getEl();
    var ddHelper = this._ddHelper;
    var containerDim = ddHelper.container.dimension;
    var maskDim = this._panelDim;

    // this is the offset we actually can expand on top/left
    // as well as on bottom/right. This algorithm will make
    // sure the mask stays a square.
    var offsetTopLeft = [
      maskDim.left, 
      maskDim.top, 
      offsetDesired 
    ].sort(this._cropper._numberComparator)[0];
    var offsetBottomRight = [
      containerDim.width - maskDim.left - maskDim.width,
      containerDim.height - maskDim.top - maskDim.height,
      offsetDesired
    ].sort(this._cropper._numberComparator)[0];

    // calculate the new dimensions
    var newDim = {};
    
    if( ddHelper.shiftKeyDown) {
      newDim = {
        top: maskDim.top - offsetTopLeft,
        left: maskDim.left - offsetTopLeft,
        width: maskDim.width + offsetTopLeft + offsetBottomRight,
        height: maskDim.height + offsetTopLeft + offsetBottomRight
      };
    }
    else {
      newDim = {
        top: maskDim.top,
        left: maskDim.left,
        width: maskDim.width + offsetBottomRight,
        height: maskDim.height + offsetBottomRight
      };
    }
    
    if( newDim.width < ddHelper.minWidth || newDim.height < ddHelper.minHeight)
      return;
      
    ddHelper.setElementDimensions( canvasMask, newDim);  
    
    // finally adjust the preview panel
    this._cropper._previewAdjuster.adjust();
  };
  
  // -----------------------------------------
  // Drag & Drop Move class
  // -----------------------------------------

  /**
   * Extend DD for moving the mask
   * @param ddHelper drag-drop help object which contains all necessary info
   *   and convenience functions
   * @constructor
   */
  Lui.Media.Cropper.DDMove = function(cropper, ddHelper, panelElId, sGroup, config) {
    this.init(panelElId, sGroup, config);
    this._cropper = cropper;
    this._ddHelper = ddHelper;
    return this;
  };
  YAHOO.extend(Lui.Media.Cropper.DDMove, YAHOO.util.DD);
  
  /**
   * The onmousedown event handler captures the current
   * mask dimensions as well as the current cursor position
   * so that we can calculate how far we have to move the
   * mask.
   */
  Lui.Media.Cropper.DDMove.prototype.onMouseDown = function(e) {
    var panel = this.getEl();
    this._panelDim = this._ddHelper.getElementDimensions( panel);
                     
    this.startWidth = panel.offsetWidth;
    this.startHeight = panel.offsetHeight;
    this.startPos = [YAHOO.util.Event.getPageX(e),
                     YAHOO.util.Event.getPageY(e)];
  };
  
  /**
   * Event handler for moving the cropping area. The
   * function makes sure that the mask cannot get outside
   * the image boundaries.
   */
  Lui.Media.Cropper.DDMove.prototype.onDrag = function(e) {
    var newPos = [YAHOO.util.Event.getPageX(e),
                  YAHOO.util.Event.getPageY(e)];
  
    var offsetX = newPos[0] - this.startPos[0];
    var offsetY = newPos[1] - this.startPos[1];

    // make sure we do not move outside the image
    var containerDim = this._ddHelper.container.dimension;
    var maxOffsetX = containerDim.width 
      - this._panelDim.left 
      - this._panelDim.width;
    if( offsetX > maxOffsetX)
      offsetX = maxOffsetX;

    var maxOffsetY = containerDim.height 
      - this._panelDim.top 
      - this._panelDim.height;
    if( offsetY > maxOffsetY)
      offsetY = maxOffsetY;

    var newTop = this._panelDim.top + offsetY;
    var newLeft = this._panelDim.left + offsetX;
    
    if( newTop < 0)
      newTop = 0;
    if( newLeft < 0)
      newLeft = 0;
      
    var panel = this.getEl();
    this._ddHelper.setElementPosition(panel,{top: newTop, left: newLeft});
    
    // finally adjust the preview panel
    this._cropper._previewAdjuster.adjust();
  };

  // -----------------------------------------
  // Preview Adjuster class
  // -----------------------------------------

  /**
   * Construct preview image adjuster.
   * @param cropper cropper instance
   * @param ddHelper ddHelper instance
   * @param image image uploaded
   * @constructor
   */
  Lui.Media.Cropper.PreviewAdjuster = function(cropper, ddHelper, image) {
    this._cropper = cropper;
    this._ddHelper = ddHelper;
    this._image = image;
    this._previewImg = YDom.get( cropper.PREVIEW_IMG_ID);
    this._previewImg.src = image.src;

    this._canvasMask = cropper._canvasMask;
    this._containerDim = ddHelper.container.dimension;
    
    this._previewWidth = cropper.pictureWidth;
  };

  /**
   * Adjust preview image based upon mask size and position. 
   */
  Lui.Media.Cropper.PreviewAdjuster.prototype.adjust = function() {
    var maskDim = this._ddHelper.getElementDimensions(this._canvasMask);
    
    var scaleFactor = this._previewWidth / maskDim.width;
    var previewImgWidth = Math.round( scaleFactor * this._containerDim.width);
    var previewImgHeight = Math.round( scaleFactor * this._containerDim.height);
    var previewImgTop = Math.round( scaleFactor * maskDim.top);
    var previewImgLeft = Math.round( scaleFactor * maskDim.left);
    this._previewImg.style.width = previewImgWidth + "px";
    this._previewImg.style.height = previewImgHeight + "px";
    this._previewImg.style.top = "-" + previewImgTop + "px";
    this._previewImg.style.left = "-" + previewImgLeft + "px";
  };
}


